{
   "action": {
      "default_popup": "./popup.html"
   },
   "content_scripts": [
      {
         "js": [
            "./content.js"
         ],
         "matches": [
            "http://*/*",
            "https://*/*"
         ],
         "run_at": "document_end"
      }
   ],
   "icons": {
      "16": "./icons/icon16.png",
      "32": "./icons/icon32.png",
      "48": "./icons/icon48.png",
      "128": "./icons/icon128.png"
   },
   "manifest_version": 3,
   "name": "Chrome Extension Template <change me>",
   "options_page": "options.html",
   "permissions": [
      "activeTab",
      "tabs",
      "storage"
   ],
   "version": "0.0.0"
}
